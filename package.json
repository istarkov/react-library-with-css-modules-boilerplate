{
  "name": "my-library-boilerplate",
  "version": "0.0.1",
  "main": "lib/index",
  "description": ".",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/library-boilerplate.js && NODE_ENV=production webpack src/index.js dist/library-boilerplate.min.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src test examples",
    "_prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd",
    "start": "babel-node devServer",
    "_test": "NODE_ENV=eee mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "library"
