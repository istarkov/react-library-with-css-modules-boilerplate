  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/istarkov",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.8.0",
    "expect": "^1.11.1",
    "jsdom": "^7.1.1",
    "koa": "^1.1.2",
    "koa-compose": "^2.3.0",
